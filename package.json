{
  "name": "bicycle-touring-companion",
  "version": "1.0.0",
  "description": "Bicycle Touring Companion",
  "scripts": {
    "build": "browserify src/js/app.js -o www/bundle.js",
    "debug:android-device": "cordova build android --device",
    "debug:android-sim": "cordova build android",
    "debug:ios-device": "cordova build ios --device",
    "debug:ios-sim": "cordova build ios",
    "dev": "shell-exec --colored-output \"npm run watch\" \"npm run livereload\"",
    "doc": "docco src/js/*.js src/js/*/*.js -o docs/generated",
    "format": "esformatter --diff \"src/js/**/*.js\"",
    "format:replace": "esformatter -i \"src/js/**/*.js\"",
    "lint": "eslint \"src/js/**/*.js\"",
    "livereload": "livereload \"platforms/browser/www/\"",
    "postbuild": "cp -r src/index.html src/img src/fonts www/",
    "postdebug:android-device": "cp \"./platforms/android/build/outputs/apk/android-debug.apk\" \"./bicycle-touring-companion-debug.apk\"",
    "postdebug:android-sim": "cp \"./platforms/android/build/outputs/apk/android-debug.apk\" \"./bicycle-touring-companion-debug.apk\"",
    "postinstall": "cordova prepare",
    "postrelease:android-device": "cp \"./platforms/android/build/outputs/apk/android-release-unsigned.apk\" \"./bicycle-touring-companion.apk\"",
    "postrelease:android-sim": "cp \"./platforms/android/build/outputs/apk/android-release-unsigned.apk\" \"./bicycle-touring-companion.apk\"",
    "prestart:android-device": "env NODE_ENV=production npm run build",
    "prestart:android-sim": "npm run build",
    "prestart:browser": "npm run build",
    "prestart:ios-device": "env NODE_ENV=production npm run build",
    "prestart:ios-sim": "npm run build",
    "release:android-device": "cordova build android --release --device",
    "release:android-sim": "cordova build android --release",
    "release:browser": "cordova build browser",
    "release:ios-device": "cordova build ios --release --device",
    "release:ios-sim": "cordova build ios --release",
    "start:android-device": "cordova run android --device",
    "start:android-sim": "cordova run android",
    "start:browser": "cordova serve 8000",
    "start:ios-device": "cordova run ios --device",
    "start:ios-sim": "cordova run ios",
    "test": "karma start",
    "watch": "nodemon -e js,html,css --watch \"src\" --exec \"npm run start:browser\""
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-css",
      "envify",
      "config-browserify"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": false,
    "rootDir": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tour-de-force/btc-app.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tour-de-force/btc-app/issues"
  },
  "homepage": "https://github.com/tour-de-force/btc-app#readme",
  "dependencies": {
    "blob-util": "~1.2.1",
    "bowser": "~1.6.0",
    "btc-models": "https://github.com/tour-de-force/btc-models#built",
    "classnames": "~2.2.5",
    "docuri": "~4.2.2",
    "history": "~4.5.1",
    "immutable": "~3.8.1",
    "leaflet": "~1.0.3",
    "lodash": "~4.17.4",
    "material-ui": "~0.17.0",
    "moment-timezone": "~0.5.11",
    "ngeohash": "~0.6.0",
    "object-assign": "~4.1.1",
    "pouchdb": "~6.1.2",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-hammerjs": "~0.5.0",
    "react-leaflet": "~1.1.0",
    "react-js-pagination": "~2.0.2",
    "react-redux": "~5.0.3",
    "react-router": "~3.0.2",
    "react-tap-event-plugin": "~2.0.1",
    "redux": "~3.6.0",
    "redux-persist": "~4.5.0",
    "redux-thunk": "~2.2.0",
    "superagent": "~3.5.0",
    "to-id": "~1.0.5",
    "underscore": "~1.8.3",
    "uuid": "~3.0.1"
  },
  "devDependencies": {
    "babel": "~6.23.0",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.23.0",
    "babelify": "~7.3.0",
    "browserify": "~14.1.0",
    "browserify-css": "~0.10.0",
    "cash-cp": "~0.2.0",
    "chai": "~3.5.0",
    "chai-jquery": "~2.0.0",
    "config": "~1.25.1",
    "config-browserify": "~1.0.5",
    "cordova": "~6.5.0",
    "docco": "~0.7.0",
    "envify": "~4.0.0",
    "es5-shim": "~4.5.9",
    "es6-shim": "~0.35.3",
    "esdoc": "~0.5.2",
    "esformatter": "~0.10.0",
    "esformatter-ignore": "~0.1.3",
    "esformatter-jsx": "~7.4.1",
    "esformatter-line-endings": "~1.0.1",
    "eslint": "~3.16.1",
    "eslint-plugin-react": "~6.10.0",
    "istanbul": "~0.4.5",
    "js-yaml": "~3.8.1",
    "karma": "~1.5.0",
    "karma-browserify": "~5.1.1",
    "karma-es6-shim": "~1.0.0",
    "karma-jquery": "~0.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.2",
    "karma-phantomjs-launcher": "~1.0.2",
    "livereload": "~0.6.0",
    "mocha": "~3.2.0",
    "nodemon": "~1.11.0",
    "phantomjs-prebuilt": "~2.1.14",
    "react-addons-test-utils": "~15.4.2",
    "redux-devtools": "~3.3.2",
    "shell-executor": "~4.0.0",
    "sinon": "~1.17.7",
    "skin-deep": "https://github.com/tour-de-force/skin-deep",
    "watchify": "~3.9.0"
  },
  "optionalDependencies": {
    "ios-deploy": "~1.9.1"
  }
}
